{
   "athicudi":[
   {"num": 1 , "poem": "அறம் செய விரும்பு" , "def": "நீ தருமம் செய்ய ஆசைப்படு." },
   {"num": 2 , "poem": "ஆறுவது சினம்"  , "def": "கோபம் தணியத் தகுவதாம்." }, 
   {"num": 3 , "poem": "இயல்வது கரவேல்" , "def": "கொடுக்க முடிந்த பொருளை இரப்பவர்க்கு ஒளியாமல் கொடு." },
   {"num": 4 , "poem": "ஈவது விலக்கேல்"	 , "def": " ஒருவர் மற்றொருவர்க்குக் கொடுப்பதைக் கொடுக்க வேண்டாமென்று நீ தடுக்காதே." },
   {"num": 5 , "poem":  "உடையது விளம்பேல்", "def": " உன்னுடைய பொருளைப் பிறர் அறியும்படி சொல்லாதே.
    உன்னுடைய பொருளை அல்லது கல்வி முதலிய சிறப்பை நீயே புகழ்ந்து பேசவேண்டா." }, 
   {"num": 6 , "poem": "ஊக்கமது கைவிடேல்" , "def": "நீ எத்தொழில் செய்யும்பொழுதும் மனவலிமை யினைக் கைவிடாதே." },  
   {"num": 7 , "poem": "எண்ணெழுத் திகழேல்"  , "def": "கணிதத்தையும், இலக்கணத்தையும் இகழாமல் நன்றாகக் கற்றுக்கொள்." },
   {"num": 8 , "poem": "ஏற்ப திகழ்ச்சி"  , "def": "இரந்துண்டு வாழ்வது பழிப்பாகையால் நீ ஒருவரிடத்தும் சென்று ஒன்றை வேண்டாதே." }, 
   {"num": 9 , "poem": "ஐய மிட்டுண்" , "def": " ஏழைகட்கும், குருடர் முடவர் முதலானவர்கட்கும் பிச்சையிட வேண்டும்."},
   {"num": 10 , "poem": "ஒப்புர வொழுகு" , "def":"உலகத்தோடு பொருந்த நடந்துகொள்." },
   {"num": 11 , "poem": "ஓதுவ தொழியேல்" , "def": "அறிவு தரும் நல்ல நூல்களை நீ எப்பொழுதும் படித்துக்கொண்டிரு." }, 
   {"num": 12 , "poem": "ஒளவியம் பேசேல்" , "def": "நீ ஒருவரிடத்தும் பொறாமைகொண்டு பேசாதே." },  
   {"num": 13 , "poem": "அஃகஞ் சுருக்கேல்" , "def": "மிகுந்த இலாபத்துக்கு ஆசைப்பட்டுத் தானியங்களைக் குறைத்து விற்காதே."},
   {"num": 14 , "poem": "கண்டொன்று சொல்லேல்"  , "def": "கண்ணாற் கண்டதற்கு மாறாகச் சொல்லாதே." }, 
   {"num": 15 , "poem": "ஙப்போல் வளை"  , "def": " ஙஎன்னும் எழுத்தானது தான்பயனுடையதாயிருந்து பயனில்லாத ஙா முதலிய தன் வருக்க எழுத்துகளைத் தழுவிக் கொள்ளுதல் போல, நீ பயனுடையவனாயிருந்து உன் இனத்தார் பயனில்லாதவராயினும் அவரைத் தழுவிக்கொள்." },
   {"num": 16 , "poem": "சனிநீ ராடு"  , "def": "சனிக்கிழமைதோறும் எண்ணெய் தேய்த்து முழுகு." },
   {"num": 17 , "poem": "ஞயம்பட வுரை"  , "def": "கேட்பவர்களுக்கு இன்ப முண்டாகும்படி இனிமை யாகப் பேசு."}, 
   {"num": 18 , "poem": "இடம்பட வீடெடேல்" , "def": "அளவுக்குமேல் இடம் வீணாய்க் கிடககும்படி வீட்டைப்பெரிதாகக் கட்டாதே. ''சிறுகக் கட்டிப் பெருக வாழ்'' என்பது பழமொழி." },  
   {"num": 19 , "poem": "இணக்கமறிந் திணங்கு"   , "def": "நற்குண நற்செய்கை உடையவ ரென்பது தெரிந்து கொண்டு ஒருவரோடு நட்புச் செய." },
   {"num": 20 , "poem": "தந்தைதாய்ப் பேண்" , "def": "உன் தாய் தந்தையரை அன்புடன் போற்றிக் காப்பாற்று." }, 
   {"num": 21 , "poem": "நன்றி மறவேல்"  , "def": "உனக்குப் பிறர் செய்த நன்மையை எப் பொழுதும் மறக்காமல் தீமையை மறந்துவிடு. 
    உதவி செய்தவர்க்கு ஒருபொழுதும் தீமை செய்தலாகாது." },
   {"num": 22 , "poem": "பருவத்தே பயிர்செய்"  , "def": "விளையும் பருவமறிந்து பயிரிடு. 
    எச்செயலும் அதற்குரிய காலத்திலே செய்யப்படவேண்டும்." },
   {"num": 23 , "poem": "மன்றுபறித் துண்ணேல்"  , "def": "நீதிமன்றத்திலே இருந்துகொண்டு இலஞ்சம் வாங்கி வாழாதே.
    'மண்பறித் துண்ணேல்' என்று பாடமிருந்தால் பிறர் நிலத்தைக் கவர்ந்து வாழாதே என்று பொருளாம்."}, 
   {"num": 24 , "poem": "இயல்பலா தனசெயேல்"  , "def": "நல்லொழுக்கத்திற்கு மாறான செயல்களைச் செய்யாதே." }, 
   {"num": 25 , "poem": "அரவ மாட்டேல்"  , "def": "பாம்பைப் பிடித்து ஆட்டி விளையாடாதே."},
   {"num": 26 , "poem": "இலவம்பஞ்சிற் றுயில்"  , "def": "இலவம்பஞ்சினாற் செய்த மெத்தையிலே படுத்து உறங்கு." }, 
   {"num": 27 , "poem": "வஞ்சகம் பேசேல்" , "def": "கபடச் சொற்களைப் பேசாதே." },
   {"num": 28 , "poem": "அழகலா தனசெயேல்" , "def": "இழிவான செயல்களைச் செய்யாதே."},
   {"num": 29 , "poem": "இளமையிற் கல்"  , "def": "இளமைப் பருவத்திலேயே படிக்கத்தொடங்கிக் கல்வியைக் கற்றுக்கொள்."}, 
   {"num": 30 , "poem": "அறனை மறவேல்"  , "def": "தருமத்தை எப்பொழுதும் மறவாமல் செய்." },  
   {"num": 31 , "poem": "அனந்த லாடேல்" , "def": "மிகுதியாகத் தூங்காதே." },
   {"num": 32 , "poem": "கடிவது மற" , "def": "யாரையும் கோபத்தாற் கடிந்து பேசாதே." }, 
   {"num": 33 , "poem": "காப்பது விரதம்" , "def": "பிற உயிர்களுக்குத் துன்பஞ் செய்யாமல் (அவற்றைக்) காப்பாற்றுவதே தவமாகும்.
    தான் செய்யத் தொடங்கிய தருமத்தை விடாமற் செய்வதே விரதம் என்றும் பொருள் சொல்லலாம்." },
   {"num": 34 , "poem": " கிழமைப் படவாழ்" , "def": "உன் உடம்பாலும் பொருளாலும் பிறருக்கு நன்மை செய்து வாழு." },
   {"num": 35 , "poem": "கீழ்மை யகற்று" , "def": "இழிவான குணஞ் செயல்களை நீக்கு." }, 
   {"num": 36 , "poem": "குணமது கைவிடேல்"  , "def": " நற்குணங்களைக் கைசோரவிடாதே. நன்மை தருவ தென்று கண்டறிந்ததைக் கைவிடாதே. அது: பகுதிப்பொருள் விகுதி." },  
   {"num": 37 , "poem": "கூடிப் பிரியேல்" , "def": "நல்லவரோடு நட்புச் செய்து பின்பு அவரை விட்டுப் பிரியாதே." },
   {"num": 38 , "poem": "கெடுப்ப தொழி" , "def": "பிறருக்குக் கெடுதி செய்வதை விட்டுவிடு (கேடு விளைக்கும் காரியத்தைச் செய்யாதே)." }, 
   {"num": 39 , "poem": "கேள்வி முயல்" , "def": "கற்றறிந்தவர்கள் சொல்லும் நூற் பொருளைக்கேட்க முயற்சி செய்."},
   {"num": 40 , "poem":  "கைவினை கரவேல்", "def": " உனக்குத் தெரிந்த கைத்தொழிலை மற்றவர்களுக்கு ஒளியாமற் செய் (ஏதேனும் கைத்தொழில் செய்துகொண்டிரு)."},
   {"num": 41 , "poem": "கொள்ளை விரும்பேல்"  , "def": "பிறர் பொருளைக் கவர்ந்துகொள்ள விரும்பாதே."}, 
   {"num": 42 , "poem": "கோதாட் டொழி" , "def": "குற்றமான விளையாட்டை விட்டுவிடு." },  
   {"num": 43 , "poem": "சக்கர நெறிநில்" , "def": "அரசன் கட்டளை வழியில் அடங்கி நட." },
   {"num": 44 , "poem": "சான்றோ ரினத்திரு."  , "def": "அறிவொழுக்கங்களில் நிறைந்த பெரியோர்களின் கூட்டத்தைச் சேர்ந்திரு." }, 
   {"num": 45 , "poem": "சித்திரம் பேசேல்" , "def": "பொய் வார்த்தைகளை மெய்போலப் பேசாதே." },
   {"num": 46 , "poem": "சீர்மை மறவேல்" , "def": "புகழுக்குக் காரணமானவற்றை மறந்துவிடாதே." },
   {"num": 47 , "poem": "சுளிக்கச் சொல்லேல்"  , "def": "கேட்பவர்க்குக் கோபமும் வெறுப்பும் உண்டாகும் படி பேசாதே" }, 
   {"num": 48 , "poem": "சூது விரும்பேல்" , "def": "ஒருபொழுதும் சூதாடுதலை விரும்பாதே."},
   {"num": 49 , "poem": "செய்வன திருந்தச்செய்"  , "def": "செய்யுஞ் செயல்களைத், திருத்தமாகச் செய்." },
   {"num": 50 , "poem": "சேரிடமறிந்து சேர்" , "def": "சேரத்தக்க நல்லிடத்தை ஆராய்ந்தறிந்து சேர்." }, 
